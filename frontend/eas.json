{
  "cli": {
    "version": ">= 15.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "credentialsSource": "local",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "credentialsSource": "local",
      "channel": "preview",
      "android": {
        "buildType": "apk"
      },
      "node": "20.18.0",
      "env": {
        "EAS_BUILD_RUNNER": "eas-build",
        "EAS_NO_BUN": "1"
      }
    },
    "production": {
      "autoIncrement": true,
      "credentialsSource": "local",
      "channel": "production",
      "android": {
        "buildType": "app-bundle"
      },
      "node": "20.18.0",
      "env": {
        "EAS_BUILD_RUNNER": "eas-build",
        "EAS_NO_BUN": "1"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-play-key.json",
        "track": "internal"
      }
    }
  }
}
